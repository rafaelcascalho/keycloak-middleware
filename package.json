{
  "name": "keycloak-middleware-ts",
  "version": "1.1.6",
  "description": "Keycloak minimalist middleware connector for backend services.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --build",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "test": "node japaFile.js",
    "test:cov": "nyc --reporter=lcov npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelcascalho/keycloak-middleware"
  },
  "keywords": [
    "keycloak",
    "auth",
    "oauth",
    "jwt",
    "rest",
    "typescript",
    "ts"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "!test/*.spec.ts"
  ],
  "author": {
    "name": "Rafael Cascalho <cascalhoeng@gmail.com>",
    "email": "cascalhoeng@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaelcascalho/keycloak-middleware/issues"
  },
  "homepage": "https://github.com/rafaelcascalho/keycloak-middleware#readme",
  "dependencies": {
    "axios": "^1.2.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "japa": "^4.0.0",
    "nyc": "^15.1.0",
    "standard": "^17.0.0",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.4"
  },
  "ts-standard": {
    "ignore": [
      "dist"
    ],
    "envs": [
      "node"
    ],
    "report": "stylish",
    "extensions": ".ts,.js"
  }
